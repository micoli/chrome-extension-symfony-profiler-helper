{
  "name": "Symfony-Profiler-Helper",
  "version": "0.0.1",
  "description": "Log and display each request containing X-Debug-Link",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/micoli/chrome-extension-symfony-profiler-helper.git"
  },
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:watch": "cross-env __DEV__=true vite build --watch",
    "build:hmr": "rollup --config utils/reload/rollup.config.ts",
    "build:extension": "node utils/packager/package-extension.js",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "npm run build:hmr && (run-p wss build:watch)",
    "test": "jest"
  },
  "type": "module",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@mantine/core": "^6.0.13",
    "@mantine/form": "^6.0.13",
    "@mantine/hooks": "^6.0.13",
    "@tabler/icons-react": "^2.22.0",
    "@webext-core/messaging": "^1.4.0",
    "@webext-core/storage": "^1.1.2",
    "curl-generator": "^0.3.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "mantine-datatable": "^2.5.9",
    "mantine-react-table": "^1.0.0-beta.8",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-json-pretty": "^2.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "@testing-library/react": "13.4.0",
    "@types/chrome": "0.0.224",
    "@types/jest": "29.5.2",
    "@types/node": "18.15.11",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.4",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.38.1",
    "@vitejs/plugin-react": "4.0.3",
    "archiver": "^5.3.1",
    "chokidar": "^3.5.3",
    "chrome-webstore-upload": "^1.0.0",
    "cross-env": "^7.0.3",
    "eslint": "8.45.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "fs-extra": "11.1.0",
    "glob": "^10.3.3",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "rollup": "2.79.1",
    "sass": "1.62.1",
    "ts-jest": "29.0.2",
    "ts-loader": "9.4.2",
    "typescript": "4.8.3",
    "vite": "4.4.6",
    "ws": "8.13.0"
  }
